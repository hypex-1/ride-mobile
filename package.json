{
  "name": "ridemobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPatterns=integration",
    "test:unit": "jest --testPathPatterns=__tests__ --testPathIgnorePatterns=integration",
    "test:components": "jest --testPathPatterns=components/__tests__",
    "test:services": "jest --testPathPatterns=services/__tests__",
    "test:contexts": "jest --testPathPatterns=contexts/__tests__",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:ci": "jest --coverage --watchAll=false --testPathIgnorePatterns=integration",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "backend:test": "node testPayments.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.8",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.27",
    "axios": "^1.12.2",
    "expo": "~54.0.10",
    "expo-device": "~8.0.9",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~14.0.7",
    "openapi-typescript": "^7.9.1",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-maps": "^1.20.1",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@jest/types": "^30.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.12",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "^29.4.4",
    "typescript": "~5.9.2"
  },
  "private": true
}
